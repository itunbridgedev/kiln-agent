{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev src/index.ts",
    "test": "cross-env NODE_ENV=test ts-node-dev src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "heroku-postbuild": "npm run build && npx prisma generate",
    "studio": "cross-env NODE_ENV=development prisma studio --hostname 0.0.0.0 --port 5555",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "db:seed:dev": "heroku run npx prisma db seed --app kilnagent-dev-api",
    "db:seed:staging": "heroku run npx prisma db seed --app kilnagent-staging-api",
    "db:reset:dev": "heroku pg:reset DATABASE --app kilnagent-dev-api --confirm kilnagent-dev-api && heroku run npx prisma migrate deploy --app kilnagent-dev-api && npm run db:seed:dev",
    "db:backup:prod": "./scripts/backup-db.sh",
    "schedule:extend": "ts-node scripts/extend-schedule-sessions.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.19.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.3",
    "@types/passport": "^1.0.17",
    "@types/passport-apple": "^2.0.3",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.11.10",
    "@types/stripe": "^8.0.416",
    "bcryptjs": "^3.0.3",
    "connect-pg-simple": "^10.0.0",
    "cookie-parser": "^1.4.7",
    "cookie-signature": "^1.2.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "jsonwebtoken": "^9.0.3",
    "passport": "^0.7.0",
    "passport-apple": "^2.0.2",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "prisma": "^6.19.1",
    "rrule": "^2.8.1",
    "stripe": "^20.3.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.1",
    "@types/cookie-signature": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "playwright": "^1.58.1",
    "prettier": "^3.7.4",
    "ts-node-dev": "^2.0.0",
    "typescript-eslint": "^8.52.0"
  }
}
